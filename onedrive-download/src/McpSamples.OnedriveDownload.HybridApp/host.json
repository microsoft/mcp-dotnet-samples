{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": ""
    }
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "dotnet",
      "workingDirectory": "",
      "arguments": [
        "McpSamples.OnedriveDownload.HybridApp.dll"
      ]
    },
    "enableForwardingHttpRequest": true,
    "enableHttpProxyingRequest": true
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}
