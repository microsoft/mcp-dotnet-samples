{
  "servers": {
    "openapi-to-sdk": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "${workspaceFolder}/openapi-to-sdk/workspace:/app/workspace",
        "-e",
        "HOST_ROOT_PATH=${workspaceFolder}/openapi-to-sdk",
        "openapi-to-sdk:latest"
      ]
    }
  }
}